type AddressResult = variant {
    Ok : text;
    Err : text;
};

type GenericResult = variant {
    Ok;
    Err : text;
};

service : (principal) -> {
    "address" : () -> (AddressResult);
    "create_certificate" : (text, text) -> (GenericResult);
    "deploy" : () -> (GenericResult);
    "close_deployment" : (nat64) -> (GenericResult);
};
