{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "rust",
      "gzip": true
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "frontend": {
        "entrypoint": "frontend/out/index.html"
      },
      "source": [
        "frontend/out"
      ],
      "type": "assets",
      "build": [
        "./scripts/build-frontend.sh"
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}